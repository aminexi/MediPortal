@model IEnumerable<HOPITAL2.Models.Examan>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TypeExamen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateExamen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Resultats)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DossierMedical)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TypeExamen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateExamen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Resultats)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DossierMedical.DossierMedicalId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ExamenId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ExamenId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ExamenId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
